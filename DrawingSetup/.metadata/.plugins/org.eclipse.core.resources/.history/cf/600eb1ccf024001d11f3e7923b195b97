	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//"sphere"
				g.drawOval(0, 0, 200, 200);
				g.drawOval(25 ,0, 200-50,200);
				g.drawOval(50, 0, 100, 200);
				g.drawOval(75, 0, 50, 200);
